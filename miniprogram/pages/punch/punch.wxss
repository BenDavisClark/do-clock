.punch{
  padding:24px;
}
.punch-btn{
  background-color:#409eff!important;
  border-color:#409eff!important;
  color:#fff;
  border-radius: 50%;
  width:100px;
  height:100px;
  font-size: 16px;
  display:flex;
  align-items: center;
  position: absolute;
  left:50%;
  top:150px;
  transform: translateX(-50%);
}
.punch-btn-hover{
  background-color: #66b1ff;
  border-color:#66b1ff;
}
.punch-head{
  height:30px;
  line-height: 30px;
  float:left;
  width:100%;
  font-size:14px;
}
.punch-head-left{
  float:left;
}
.punch-head-right{
  float:right;
}
.punch-addr{
  font-size:14px;
}

.getUserInfo{
  width: 90px;
  font-size: 14px;
}
.record_list{
    float:left;
    height:35px;
    line-height: 35px;
    position: absolute;
    top:280px;
    font-size:14px;
}
.record_list_addr{
  width:100%;
  float:left;
}
.record_index{
  color:#409eff;
  font-weight: 600px;
}
